    xmlns:animations="clr-namespace:Rg.Plugins.Popup.Animations;assembly=Rg.Plugins.Popup"
    xmlns:local="clr-namespace:BitcoinWalletApp.Views"
    x:Class="BitcoinWalletApp.Views.Popups.ChangeCoin_Popup">
